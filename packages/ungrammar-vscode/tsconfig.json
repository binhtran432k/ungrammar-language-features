{
	"compilerOptions": {
		"rootDir": "./src",
		"skipLibCheck": true,
		"lib": ["es2020", "webworker"],
		"types": ["vscode"],
		"allowJs": true,
		"noEmit": true,
		"strict": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"declaration": true,
		"stripInternal": true,
		"sourceMap": true,
		"declarationMap": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false
	},
	"include": ["./src"]
}
